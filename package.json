{
  "name": "@feryardiant/sendgrid-inbound-parser",
  "type": "module",
  "version": "0.0.10",
  "description": "Simple library to parse SendGrid Inbound Mail",
  "author": "Fery Wardiyanto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feryardiant/sendgrid-inbound-parser.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "test": "vitest run",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@types/busboy": "^1.5.3",
    "@types/mailparser": "^3.4.4",
    "busboy": "^1.6.0",
    "mailparser": "^3.7.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@firebase/logger": "^0.3.4",
    "@google-cloud/storage": "^7.10.0",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "express": "^4.19.2",
    "simple-git-hooks": "^2.11.1",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.4",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit $1",
    "pre-commit": "pnpm exec lint-staged --allow-empty"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ],
    "*.json": [
      "eslint --fix"
    ]
  },
  "standard-version": {}
}
